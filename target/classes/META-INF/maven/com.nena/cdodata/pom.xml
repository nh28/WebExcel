<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.nena</groupId>
  <artifactId>cdodata</artifactId>
  <version>0.0.1-SNAPSHOT</version>
      
  <dependencies>
    
    <dependency>
        <groupId>org.apache.poi</groupId>
        <artifactId>poi</artifactId>
        <version>5.2.5</version> <!-- Check for the latest version on the Apache POI website -->
    </dependency>
    
    <dependency>
        <groupId>org.jsoup</groupId>
        <artifactId>jsoup</artifactId>
        <version>1.14.3</version> <!-- Use the latest version available -->
    </dependency>
    
</dependencies>

<build>
        <plugins>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>exec-maven-plugin</artifactId>
                <version>3.1.1</version>
                <configuration>
                    <!-- Specify the mainClass to run -->
                    <mainClass>cdodata.GenerateData</mainClass>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>